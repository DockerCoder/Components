source ‘https://github.com/CocoaPods/Specs.git’
platform :ios, '9.0'
inhibit_all_warnings!
target 'Components' do
   
    pod 'SDAutoLayout','2.2.1' # 自动布局
    pod 'MJExtension','3.0.17' # 字段转模型
    pod 'IQKeyboardManager','6.4.0' # 键盘工具条
    pod 'AFNetworking','3.2.1' # 网络请求
    pod 'MJRefresh','3.2.0' # 刷新组件
    pod 'TZImagePickerController','3.2.1' # 图片选择器
    pod 'FLAnimatedImage','1.0.12' # gif图片播放
    pod 'SDWebImage','5.0.6' # 网络图片异步请求框架
    pod 'SDCycleScrollView','1.80' # 轮播图
    
end
